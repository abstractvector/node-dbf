{
  "name": "node-dbf",
  "version": "0.4.0",
  "engines": {
    "node": ">=7"
  },
  "description": "An efficient dBase DBF file parser written in pure JavaScript",
  "main": "./lib/parser.js",
  "bin": "./bin/node-dbf.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/abstractvector/node-dbf.git"
  },
  "keywords": [
    "dBase",
    "dbf"
  ],
  "author": "Matt Knight <abstractvector@users.noreply.github.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "mocha": "^4.1.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "iconv-lite": "^0.6.2"
  },
  "scripts": {
    "clean": "rimraf lib && mkdir lib",
    "build-server": "babel -d ./lib ./src",
    "build": "npm run clean && npm run build-server",
    "prepare": "npm run build && npm test",
    "test": "mocha --require babel-core/register --recursive"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
